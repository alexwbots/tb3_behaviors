<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Go_to_goal">

	<executable package_path="tb3_flexbe_behaviors.go_to_goal_sm" class="Go_to_goalSM" />
	<tagstring></tagstring>
	<author>Alexander Lopez</author>
	<date>Wed Dec 01 2021</date>
	<description>
		This behavior makes the robot go to a position and orientation in static enviroment
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="lineal1" default="1.0" label="lineal1" hint="Sets the lineal1">
			<min value="0" />
			<max value="5.0" />
		</param>

		<param type="numeric" name="angular1" default="0.75" label="angular1" hint="Sets the angular1">
			<min value="0" />
			<max value="3.1416" />
		</param>

	</params>


</behavior>